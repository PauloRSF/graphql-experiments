{
  "name": "graphql-experiments",
  "version": "0.0.1",
  "private": "true",
  "type": "module",
  "scripts": {
    "start:dev": "nodemon -e ts,json --exec 'tsx ./src/index.ts | pino-pretty -c'",
    "predata:seed": "mkdir -p data && sqlite3 data/users.db < src/database/schema.sql",
    "data:seed": "tsx ./src/seed.ts",
    "data:clean": "rimraf ./data"
  },
  "dependencies": {
    "@apollo/server": "4.10.2",
    "better-sqlite3": "9.4.3",
    "graphql": "16.8.1",
    "knex": "3.1.0",
    "pino": "8.19.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/node": "^20.11.30",
    "fishery": "^2.2.2",
    "listr2": "^8.0.2",
    "nodemon": "^3.1.0",
    "pino-pretty": "^11.0.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.1"
  }
}