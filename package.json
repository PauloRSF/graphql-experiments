    "pino": "8.19.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/better-sqlite3": "^7.6.9",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.30",
    "fishery": "^2.2.2",
    "listr2": "^8.0.2",
    "lodash": "^4.17.21",
